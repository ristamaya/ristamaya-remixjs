datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model users {
  userid       String    @id @default(auto()) @map("_id") @db.ObjectId
  useremail    String    @unique
  userpassword String
  userstatus   String    @default("active")
  maxsession   BigInt?
  createby     String?
  createat     DateTime  @default(now())
  updateat     DateTime? @updatedAt
}

model menus {
  menuid   String @id @default(auto()) @map("_id") @db.ObjectId
  group    String
  parent   String
  type     String
  title    String
  icon     String
  path     String
  pathtype String
  order    Int
  status   String
  createby String
}

model visitors {
  visitorid    String   @id @default(auto()) @map("_id") @db.ObjectId
  IPv4         String?
  city         String?
  country_code String?
  country_name String?
  latitude     String?
  longitude    String?
  postal       String?
  state        String?
  visitat      DateTime @default(now())
}

type peopleid {
  idtype String?
  idno   String?
}

model peoples {
  peopeid   String     @id @default(auto()) @map("_id") @db.ObjectId
  firstname String?
  midname   String?
  lastname  String?
  nickname  String?
  address   String?
  provinsi  String?
  kota      String?
  kecamatan String?
  kelurahan String?
  kodepos   String?
  identitas peopleid[]
}
