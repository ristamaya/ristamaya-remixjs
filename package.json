{
  "name": "remix-template-remix",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && cross-env NODE_ENV=production remix build",
    "build:css": "tailwindcss -m -i ./tailwind.css -o app/tailwind.css",
    "dev": "concurrently \"npm run dev:css\" \"cross-env NODE_ENV=development netlify dev\"",
    "dev:css": "tailwindcss -w -i ./tailwind.css -o app/tailwind.css",
    "start": "cross-env NODE_ENV=production netlify dev"
  },
  "dependencies": {
    "@netlify/functions": "^0.10.0",
    "@node-rs/bcrypt": "^1.6.0",
    "@prisma/client": "^3.13.0",
    "@remix-run/netlify": "^1.4.3",
    "@remix-run/node": "^1.5.1",
    "@remix-run/react": "^1.5.1",
    "@remix-run/serve": "^1.5.1",
    "cross-env": "^7.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "tiny-invariant": "^1.2.0",
    "zustand": "^4.0.0-rc.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.5.1",
    "@remix-run/eslint-config": "^1.5.1",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.1.0",
    "eslint": "^8.15.0",
    "postcss": "^8.4.13",
    "prettier": "^2.6.2",
    "prettier-plugin-prisma": "^3.14.0",
    "prettier-plugin-tailwindcss": "^0.1.10",
    "prisma": "^3.13.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14"
  }
}
