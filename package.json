{
  "name": "remix-template-remix",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./tailwind.css -o app/tailwind.css",
    "dev": "concurrently \"npm run dev:css\" \"cross-env NODE_ENV=development remix dev\"",
    "dev:css": "tailwindcss -w -i ./tailwind.css -o app/tailwind.css",
    "start": "cross-env NODE_ENV=production remix dev"
  },
  "dependencies": {
    "@prisma/client": "^3.12.0",
    "@remix-run/node": "^1.4.0",
    "@remix-run/react": "^1.4.0",
    "@remix-run/serve": "^1.4.0",
    "cross-env": "^7.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tiny-invariant": "^1.2.0",
    "react-icons": "^4.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.4.0",
    "@remix-run/eslint-config": "^1.4.0",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.1.0",
    "eslint": "^8.11.0",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "prisma": "^3.12.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  }
}
